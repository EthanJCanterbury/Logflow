[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 17724980
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[[ports]]
localPort = 5000
externalPort = 80
